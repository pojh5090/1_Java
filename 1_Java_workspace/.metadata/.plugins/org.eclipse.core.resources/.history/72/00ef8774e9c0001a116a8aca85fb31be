package com.kh.operator;

public class A_LogicalNegation {
	public void method() {
		System.out.println("true의 부정 : " + !true);
		System.out.println("false의 부정 : " + !false);
		
		boolean bool1 = false;
		boolean bool2 = true;
		
		System.out.println("!bool1 : " + !bool1);
		System.out.println("!bool2 : " + !bool2);
		System.out.println("!!bool1 : " + !!bool1);
		System.out.println("!!bool2 : " + !!bool2);
	}
}
